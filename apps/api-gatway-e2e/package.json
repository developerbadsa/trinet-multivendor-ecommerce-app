{
  "name": "@trinet-multivendor-ecommerce-app/api-gatway-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@trinet-multivendor-ecommerce-app/api-gatway"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/api-gatway-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@trinet-multivendor-ecommerce-app/api-gatway:build",
          "@trinet-multivendor-ecommerce-app/api-gatway:serve"
        ]
      }
    }
  }
}
